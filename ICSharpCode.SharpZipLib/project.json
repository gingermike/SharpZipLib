{
  "version": "1.0.0-BP",
  "title": "SharpZipLib",
  "description": "A free C# compression library",
  "copyright": "Copyright © 2000-2016 SharpZipLib Contributors",

  "buildOptions": {
    "keyFile": "../ICSharpCode.SharpZipLib.snk",
    "xmlDoc": true
  },

  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "RELEASE", "TRACE" ],
        "optimize": true
      }
    }
  },

  "packOptions": {
    "summary": "#ziplib (SharpZipLib, formerly NZipLib) is a Zip, GZip, Tar and BZip2 library written entirely in C# for the .NET platform. It is implemented as an assembly (installable in the GAC), and thus can easily be incorporated into other projects (in any .NET language).",
    "description": "#ziplib (SharpZipLib, formerly NZipLib) is a Zip, GZip, Tar and BZip2 library written entirely in C# for the .NET platform. It is implemented as an assembly (installable in the GAC), and thus can easily be incorporated into other projects (in any .NET language).",
    "owners": [ "http://www.icsharpcode.net/" ],
    "projectUrl": "http://www.icsharpcode.net/OpenSource/SharpZipLib/Default.aspx",
    "requireLicenseAcceptance": false
  },

  "frameworks": {
    "net45": {
      "dependencies": {
      }
    },
    "netstandard1.3": {
      "buildOptions": {
        "define": [ "PCL" ]
      },
      "dependencies": {
        "System.Linq": "4.1.0",
        "System.Runtime.Extensions": "4.1.0",
        "System.Runtime.Numerics": "4.0.1",
        "System.IO": "4.1.0",
        "System.IO.FileSystem": "4.0.1",
        "System.Runtime": "4.1.0",
        "System.Runtime.Serialization.Primitives": "4.1.1",
        "System.Text.RegularExpressions": "4.1.0",
        "System.Collections": "4.0.11",
        "System.Security.Cryptography.Primitives": "4.0.0",
        "System.Security.Cryptography.Algorithms": "4.2.0",
        "System.Collections.NonGeneric": "4.0.1",
        "System.Threading": "4.0.11",
        "System.Threading.Thread": "4.0.0"
      }
    }
  }
}
